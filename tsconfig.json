{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@public/*": ["public/*"],
      "@components/*": ["src/components/*"],
      "@services/*": ["src/services/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@i18n/*": ["src/i18n/*"],
      "@astroComponents/*": ["src/components/astro/*"],
      "@reactComponents/*": ["src/components/react/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "importsNotUsedAsValues": "remove",
    "isolatedModules": false
  }
}
